{
  "name": "webcc-frontend",
  "version": "1.1.30",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx serve",
    "build": "nx build",
    "join:translations": "nx g @ngneat/transloco:build",
    "test": "npm run join:translations && nx run-many --target=test --all --code-coverage --coverageReporters=html,lcov",
    "lint": "nx run-many --all --target=lint",
    "build:stats": "npm run build -- --stats-json",
    "bundle:analyze": "webpack-bundle-analyzer dist/apps/webcc-frontend/stats.json",
    "mergeCoverageReports": "node merge-coverage-reports.js",
    "sonar": "node sonar-scanner.js",
    "prepare": "husky install",
    "coverage": "jest --collect-coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.2.0",
    "@angular/cdk": "^14.2.2",
    "@angular/common": "14.2.0",
    "@angular/compiler": "14.2.0",
    "@angular/core": "14.2.0",
    "@angular/forms": "14.2.0",
    "@angular/platform-browser": "14.2.0",
    "@angular/platform-browser-dynamic": "14.2.0",
    "@angular/router": "14.2.0",
    "@ng-web-apis/common": "^2.0.1",
    "@ng-web-apis/resize-observer": "^2.0.0",
    "@ngneat/transloco": "^4.1.1",
    "@ngrx/effects": "^14.3.0",
    "@ngrx/entity": "^14.3.0",
    "@ngrx/store": "^14.3.0",
    "@ngrx/store-devtools": "^14.3.0",
    "@nrwl/angular": "14.6.1",
    "@types/d3": "^7.4.0",
    "ag-grid-community": "^28.2.1",
    "d3": "^7.6.1",
    "keycloak-angular": "^12.1.0",
    "keycloak-js": "^19.0.3",
    "lodash": "^4.17.21",
    "ng-mocks": "^14.2.4",
    "ol": "^6.15.1",
    "primeicons": "^5.0.0",
    "primeng": "14.0.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-architects/ddd": "^2.1.2",
    "@angular-devkit/build-angular": "14.2.1",
    "@angular-eslint/eslint-plugin": "~14.0.0",
    "@angular-eslint/eslint-plugin-template": "~14.0.0",
    "@angular-eslint/template-parser": "~14.0.0",
    "@angular/cdk": "^14.2.2",
    "@angular/cli": "~14.2.0",
    "@angular/compiler-cli": "14.2.0",
    "@angular/language-service": "14.2.0",
    "@ngrx/eslint-plugin": "^14.3.0",
    "@nrwl/cli": "14.6.1",
    "@nrwl/cypress": "14.6.1",
    "@nrwl/eslint-plugin-nx": "14.6.1",
    "@nrwl/jest": "14.6.1",
    "@nrwl/linter": "14.6.1",
    "@nrwl/workspace": "14.6.1",
    "@types/jest": "28.1.8",
    "@types/lodash": "^4.14.185",
    "@types/node": "16.11.7",
    "@types/ol": "^6.5.3",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "autoprefixer": "^10.4.0",
    "cypress": "^10.2.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-deprecation": "^1.3.2",
    "husky": "^8.0.1",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.1",
    "jest-preset-angular": "12.2.2",
    "lint-staged": "^13.0.3",
    "nx": "^14.6.1",
    "postcss": "^8.4.5",
    "prettier": "^2.6.2",
    "sonarqube-scanner": "^2.8.1",
    "tailwindcss": "^3.0.2",
    "tailwindcss-themer": "^2.0.1",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "type-fest": "^2.19.0",
    "typescript": "~4.7.2",
    "webpack-bundle-analyzer": "^4.6.1"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "**/*": "prettier --write --ignore-unknown"
  }
}
